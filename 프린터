def solution(priorities, location):
    
    deque = [[i, p] for i, p in enumerate(priorities)]
    answer = 0

    while len(priorities) > 0:
        check = deque.pop(0)
        if any(check[1] < deq[1] for deq in deque):
            deque.append(check)
        else:
            answer += 1
            if check[0] == location:
                return answer



# 리스트를 enumerate를 활용하여 만드는 방법 배움.
# any, all 함수 공부함!!.