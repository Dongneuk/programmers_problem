from collections import deque

def solution(cacheSize, cities):
    answer = 0
    cache_lst = deque()
    
    if cacheSize == 0:
        return len(cities) * 5

    else:
        for city in cities:
            city = city.lower()
            
            if city in cache_lst: 
                answer += 1
                cache_lst.remove(city)
                cache_lst.append(city)
            
            else:
                answer += 5
                if len(cache_lst) >= cacheSize:
                    cache_lst.popleft()
                    cache_lst.append(city)
                else:
                    cache_lst.append(city)
    
    return answer