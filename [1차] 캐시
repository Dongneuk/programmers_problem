from collections import deque

def solution(cacheSize, cities):
    
    answer = 0
    idx = 0
    cache_lst = deque()
    
    if cacheSize == 0:
        return len(cities) * 5
    
    
    else:
        for i in cities:
            i = i.lower()
            
            if i in cache_lst:
                cache_lst.remove(i)
                cache_lst.append(i)
                answer += 1
            else:
                answer += 5
                
                if idx < cacheSize:
                    cache_lst.append(i)
                    idx += 1
                else:
                    cache_lst.popleft()
                    cache_lst.append(i)
    return answer            
    
    