def solution(N, stages):
    
    dic = {}
    n = len(stages)
    
    
    for i in range(1, N+1):
        if n != 0:
            cnt = stages.count(i)
            dic[i] = cnt / n
            n -= cnt
        else:
            dic[i] = 0
            

    dic = sorted(dic, key = lambda x: dic[x], reverse = True)
    
    
    return dic
        
## tc: O(N^2) because list.count() ê°€ O(N).
## hash table, brute force