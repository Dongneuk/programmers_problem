def solution(n, words):
    
    #사용된 단어
    used_word = set()
    used_word.add(words[0]) 
    
    # 1부터 for 문 돌려서 used_word에 있는지, last_alpha 일치하는지 확인한다.
    for i in range(1, len(words)):
        if words[i] in used_word or words[i][0] != words[i-1][-1]:
            return [(i%n) + 1, (i//n) + 1]
        else:
            used_word.add(words[i])
    
    else:
        return [0, 0]